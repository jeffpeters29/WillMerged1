@page
@model Qwill.Pages.EditModel
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h2>Edit</h2>

<div id="wrapper">
    <section>
        <div class="container">
            <div class="row">

                <div class="col-lg-6">
                    <h1>@ViewData["Title"] : @Model.WillInfo.FullName</h1>
                </div>

                <div class="col-md-6 p-3">
                    <div>
                        <partial name="_ErrorMessages" for="ErrorMessage" />
                        <partial name="_Step1" for="WillInfo" />
                    </div>
                </div>

            </div>
        </div>
    </section>
</div>